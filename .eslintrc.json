{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": ["**/dist/*.js", "**/test/*"],
    "rules": {
        "no-console": 0,
        "no-empty": 0,
        "no-irregular-whitespace":0,
        "no-mixed-requires": "off"
    }
}